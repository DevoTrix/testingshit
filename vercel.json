{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {"src":"/api/pushingToDB.py",
    "use":"@vercel/python"
  }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "crons":[
    {
      "path": "/api/pushingToDB.py",
      "schedule":"0 */6 * * *"
    }
  ]
}