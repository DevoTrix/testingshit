{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "crons": [
    {
      "name": "pullData",
      "schedule": "0 */6 * * *",
      "path": "/helperFunctions/pullData.js"
    }
  ]
  
}
